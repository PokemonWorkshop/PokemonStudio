    "yarn-deduplicate": "^3.1.0"
  },
  "dependencies": {
    "caniuse-lite": "^1.0.30001359",
    "csv-parse": "^5.0.3",
    "csv-stringify": "^6.0.5",
    "custom-electron-titlebar": "^4.0.1",
    "electron-debug": "^3.1.0",
    "electron-log": "^4.2.4",
    "electron-updater": "^4.3.4",
    "extract-zip": "^2.0.1",
    "glob": "^7.1.7",
    "history": "^5.0.0",
    "i18next": "^19.9.0",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-http-backend": "^1.1.1",
    "immutability-helper": "^3.1.1",
    "marshal": "^0.5.3",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.1",
    "react-flow-renderer": "^10.3.14",
    "react-i18next": "^11.12.0",
    "react-is": "^17.0.2",
    "react-notifications-component": "^3.1.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^5.2.1",
    "react-textarea-autosize": "^8.3.3",
    "react-tooltip": "^4.2.21",
    "react-tracked": "^1.7.4",
    "react-virtualized": "^9.22.3",
    "regenerator-runtime": "^0.13.5",
    "scheduler": "^0.20.2",
    "source-map-support": "^0.5.19",
    "styled-components": "^5.3.3",
    "typedjson": "^1.7.0"
  },
  "devEngines": {
    "node": ">=10.x",
    "npm": ">=6.x",
    "yarn": ">=1.21.3"
  },
  "browserslist": [],
  "prettier": {
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".babelrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ],
    "singleQuote": true
  },
  "renovate": {
    "extends": [
      "bliss"
    ],
    "baseBranches": [
      "next"
    ]
  },
  "resolutions": {
    "graceful-fs": "^4.2.4"
  }
}
